  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Firstname</td>
        <td class="itemValue"><%= @registration.firstname%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Lastname</td>
        <td class="itemValue"><%= @registration.lastname%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Emailid</td>
        <td class="itemValue"><%= @registration.emailid%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Confirmemailid</td>
        <td class="itemValue"><%= @registration.confirmemailid%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Companyname</td>
        <td class="itemValue"><%= @registration.companyname%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Address1</td>
        <td class="itemValue"><%= @registration.address1%></td>
      </tr>
      
      <tr>
