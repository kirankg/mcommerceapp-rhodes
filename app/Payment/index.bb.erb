<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "New", :action => :new %>
    <h1>Payments</h1>
    <%= link_to "Home", RhoConfig::start_path %>
  </div>

  <div data-role="content">
    <table>
      <tr>
      <% @payments.each do |obj| %>
        
            <td class="recordLabel"><%= link_to "#{obj.cardtype}", :action => :show, :id => obj.object %></td>
        
      <% end %>
      </tr>
    </table>
  </div>

</div>
